{
    "name": "wicle-repo",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "turbo dev --filter=@wicle/demo --filter=wicle",
        "build": "turbo run build",
        "clean": "echo clean",
        "reset": "npm-run-all -s clean reset:turbo reset:node",
        "reset:turbo": "find . -type d -name .turbo -exec rm -rf {} +",
        "reset:node": "find . -type d -name node_modules -exec rm -rf {} +"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/shnam7/wicle"
    },
    "keywords": [
        "scss",
        "utility",
        "web",
        "ui"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/shnam7/wicle/issues"
    },
    "homepage": "https://github.com/shnam7/wicle#readme",
    "devDependencies": {
        "npm-run-all": "^4.1.5"
    }
}
